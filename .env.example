# Cors
CORS_ORIGIN=http://localhost:4200

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=10

# Pagination
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100
